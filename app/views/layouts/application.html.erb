<!DOCTYPE html>
<html>
  <head>
    <title>Lab4Assignment</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar">
        <% if current_user %>
          <li><a  href="http://localhost:3000/users/sign_out" data-method="delete">Log out</a> </li>
        <% else %>
          <li><a  href="http://localhost:3000/sign_in">Log in</a></li>
          <button><%= link_to "Sign in with Google", user_google_oauth2_omniauth_authorize_path %></button>
        <% end %>
      
    </nav>
  	<p id="notice"><%= notice %></p>
    <%= yield %>

    
  </body>
</html>
