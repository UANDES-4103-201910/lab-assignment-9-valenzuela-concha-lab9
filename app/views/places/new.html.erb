<h1>New Place </h1>

<%= form_with scope: :place, url: places_path, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :address %><br>
    <%= form.text_area :address %>
  </p>

  <p>
    <%= form.label :capacity %><br>
    <%= form.text_area :capacity %>
  </p>

 
  <p>
    <%= form.submit %>
  </p>
<% end %>